def one_hot(col):
    global X
    uni=X[col].unique()
    #for i in range(len(uni)):
    #   df[col] = df[col].apply(lambda x:i if x==uni[i] else x)
    a=X[[col]]
    enc = OneHotEncoder()
    enc.fit(a)
    a= enc.transform(a).toarray()
    a=pd.DataFrame(a,columns=[col+'_'+i for i in uni])
    X= pd.concat((X, a), axis = 1)
    del X[col]


